        MENUITEM "Delete",                      IDM_DELETE
        MENUITEM SEPARATOR
        MENUITEM "Select &All",                 IDM_SELECTALL
        MENUITEM SEPARATOR
        MENUITEM "Clear",                 		IDM_CLEAR
    END
    POPUP "Tabs"
    BEGIN
//        MENUITEM SEPARATOR
        MENUITEM "Close Other Tabs",            IDM_CLOSEOTHERTABS
        MENUITEM "Close Tab",                   IDM_CLOSETAB
    END
END

